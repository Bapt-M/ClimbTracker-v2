{
  "name": "climbtracker-v2",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "eslint apps packages",
    "lint:strict": "eslint apps packages --max-warnings 0",
    "lint:fix": "eslint apps packages --fix",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "db:generate": "turbo db:generate",
    "db:migrate": "turbo db:migrate",
    "db:push": "turbo db:push",
    "db:studio": "pnpm --filter @climbtracker/database studio",
    "migrate:data": "tsx scripts/migrate-data.ts",
    "test:e2e": "node scripts/run-selenium-tests.js",
    "test:e2e:full": "node scripts/run-selenium-tests.js --full",
    "test:e2e:admin": "node scripts/run-selenium-tests.js --admin",
    "test:e2e:lambda": "node scripts/run-selenium-tests.js --lambda",
    "test:e2e:headless": "node scripts/run-selenium-tests.js --full --headless"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "tsx": "^4.19.0",
    "turbo": "^2.3.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.54.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20"
  }
}
